{
  "importMap": "import_map.json",
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "simulate": "rollup ./sim/bundle/ohhell.js -o ./sim/src/ohhell.js --format es && rollup ./sim/bundle/mexica.js -o ./sim/src/mexica.js --format es && lume --config _config.sim.js"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
